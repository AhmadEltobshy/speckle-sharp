    <LangVersion>8</LangVersion>
    <Version>2.0.0</Version>
    <PackageId>Speckle.Transports.Server</PackageId>
    <Authors>Speckle</Authors>
    <Company>Speckle</Company>
    <Product>ServerTransport</Product>
